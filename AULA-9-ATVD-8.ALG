Algoritmo "aula-9-atvd-8"

Var
   quantidade: inteiro
   aluno1: caractere
   cont: inteiro
   



Inicio

      escreval ("Qual a quantidade alunos? ")
      leia (quantidade)
      enquanto (cont <=quantidade)  faca
      escreval (cont)
      cont<- cont +1
      escreval(" Aluno  ", cont)
      escreva (" Nome do Aluno: ")
      leia (aluno1)
      fimenquanto
      



Fimalgoritmo