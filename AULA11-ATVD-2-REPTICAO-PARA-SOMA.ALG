Algoritmo "aula11-atvd-2-repticao-para-soma"

Var
   contador: inteiro
   soma: inteiro
   numero: inteiro


Inicio
   soma <- 0
   para contador <- 1 ate 10 faca
      escreval ("Digite um valor: ")
      leia (numero)
      soma <- soma + numero
   fimpara
   escreval (" A soma dos vcalores é ", soma)



Fimalgoritmo