Algoritmo "aula8-atvd3-somador-de-valores"

Var
   contador: inteiro
   soma: inteiro
   numero: inteiro


Inicio
      contador <- 1
      soma <- 1
      enquanto (contador <= 5) faca
      escreval ("Digite um valor: ")
      leia (numero)
      soma <- soma + numero
      contador <- contador +1
      fimenquanto
      escreval (" A soma dos vcalores é ", soma)



Fimalgoritmo