Algoritmo "Aula9-atvd5"

Var
   Cont, Soma, N: inteiro

Inicio
      Cont <- 1
      Soma <- 0
      enquanto   (Cont <= 5) faca
              escreva (" Digite o ", Cont, "°. valor " )
              Leia (N)
              Soma <- Soma + N
              Cont <- Cont + 1
      fimenquanto
      escreval (" A soma de todos os valore foi: ", Soma)

Fimalgoritmo